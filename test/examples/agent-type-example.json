{
    "id": "demoagenttype",
    "description": "Description of this demoagenttype",
    "partner": "guard-partner",
    "schema": "yaml",
    "source": "/etc/example_agent/config.yaml",
    "actions": [
        {
            "id": "start",
            "cmd": "systemctl start example_agent",
            "status": "started"
        },
        {
            "id": "stop",
            "cmd": "systemctl stop example_agent",
            "status": "stopped"
        }
    ],
    "parameters": [
        {
            "path": "timers.timeout",
            "description": "Timeout value in connections",
            "type": "integer"
        },
        {
            "path": "timers.time_between_probes",
            "description": "Time between different resource probes",
            "type": "integer"
        }
    ]
}
